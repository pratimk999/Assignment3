<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('../partials/head'); %>
</head>
<body>

    <header>
        <%- include('../partials/header'); %>
    </header>

    <h1><%= tagline.split(' ').join('.') %></h1>

    <ul>
        <% people.forEach(function(person) { %>
            <li>
                <strong><%= person.name %></strong>
                representing <%= person.place %>
            </li>
        <% }); %>
    </ul>

    <footer>
        <%- include('../partials/footer.ejs'); %>
    </footer>
</body>
</html>
